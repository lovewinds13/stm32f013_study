void *My_Memcpy(void *dest, const void *src, unsigned int count)
{
	char *pdest = (char *)dest;
	char *psrc = (char *)src;

	if (pdest == 0 || psrc == 0)
	{
		return 0;
	}

	if (pdest == psrc)
	{
		return 0;
	}

	if (pdest > psrc)	
	{
		while (count--)
		{
			*(pdest + count) = *(psrc + count);
		}
	}
	else
	{
		while (count--)
		{
			*pdest++ = *psrc++;
		}
	}
	return pdest;
}












