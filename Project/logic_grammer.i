uint16_t Crc_B16(uint8_t *Indata, uint16_t len, uint16_t CrcTmp)
{
	uint16_t y = 0;
	uint16_t yy = 0;
	uint8_t x = 0;
	uint32_t i = 0;
	uint32_t j = 0;

     y = ~CrcTmp & 0xffff;
	
	for(i = 0; i < len; i++)
	{
		x = Indata[i];
		for (j = 0; j < 8; j++)
		{
			if (((y^x) & 1) !=0) 
			{
				yy= 0x8408;
			}
			else 
			{
				yy=0;
			}
		x >>= 1;
		y >>= 1;
		y ^= yy;
		}
	}
	
	return (~y & 0xffff);
}

void bubble_sort(uint32_t *pbdata, uint16_t length)
{
	uint16_t i = 0;
	uint16_t j = 0;
	uint32_t temp_value = 0;
	uint8_t sort_flag = 0;
	
	for (i = 0; i < length-1; i++)
	{
		sort_flag = 1;
		
		for (j = 0; j < length-1-i; j++)
		{
			if (pbdata[j] > pbdata[j+1])
			{
				temp_value	= pbdata[j];
				pbdata[j]	= pbdata[j+1];
				pbdata[j+1]	= temp_value;
				
				sort_flag = 0;
			}
		}
		
		if (sort_flag == 1)
		{
			goto exit;
		}
	}
	
exit:	
	return;
}

